    "ioredis": "^5.7.0",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.6"
